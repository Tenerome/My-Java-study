package 课程;

import java.util.Scanner;

public class 一维数组的定义和赋值 {

	public static void main(String[] args) {
		Scanner shuru=new Scanner (System.in);
		int a[]=new int [10];
		for(int i=0;i<a.length;i++)
			a[i]=shuru.nextInt();
		System.out.print("输入的数组为:");
		for (int i:a)
			System.out.print(i+" ");

	}

}
